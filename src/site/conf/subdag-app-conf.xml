<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
  <property>
    <name>dt.attr.DEBUG</name>
    <value>false</value>
  </property>
  <property>
    <name>dt.loggers.level</name>
    <value>com.datatorrent.apps.ingestion.*:DEBUG,com.datatorrent.lib.*:DEBUG,com.datatorrent.stram.engine.*:DEBUG</value>
  </property>
  <property>
    <name>dt.attr.MASTER_MEMORY_MB</name>
    <value>1024</value>
  </property>
  <property>
    <name>dt.operator.FileSplitter.prop.directory</name>
    <value>/user/chandni/ingestion/source</value>
  </property>

  <property>
