  </property>

  <property>
    <name>dt.attr.CONTAINER_JVM_OPTIONS</name>
    <value>-XX:+HeapDumpOnOutOfMemoryError -XX:HeapDumpPath=/tmp/</value>
  </property>


  <property>
    <name>dt.operator.Console.port.input.attr.PARTITION_PARALLEL</name>
    <value>true</value>
  </property>
  <property>
    <name>dt.operator.Console.port.stringInput.attr.PARTITION_PARALLEL</name>
    <value>true</value>
  </property>
